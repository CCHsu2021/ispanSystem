@model IEnumerable<MSITTeam1.Models.TTestPaperBank>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FDesignerAccount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FTestPaperName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FSubjectId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FNote)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FDesignerAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FTestPaperName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FSubjectId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FNote)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FTestPaperId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FTestPaperId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FTestPaperId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
